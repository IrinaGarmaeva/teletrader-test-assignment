{
  "root": true,
  "plugins": ["@typescript-eslint", "import", "prettier"],
  "extends": [
    "airbnb",
    "react-app",
    "react-app/jest",
    "plugin:@typescript-eslint/recommended",
    "airbnb-typescript"
  ],
  "rules": {
    "max-len": "off",
    "@typescript-eslint/no-shadow": "off",
    "react/prop-types": "off"
  },
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "parser": "@typescript-eslint/parser"
}
